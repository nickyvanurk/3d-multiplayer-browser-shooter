{
  "name": "void",
  "version": "1.0.0",
  "description": "3D multiplayer space shooter game.",
  "main": "app.js",
  "scripts": {
    "client:start:dev": "webpack-dev-server --config client/webpack.dev.js",
    "client:build": "webpack --config client/webpack.prod.js",
    "server:start:dev": "nodemon --config server/nodemon.json --exec babel-node -- server/src/index.js",
    "server:start": "babel-node server/src/index.js",
    "postinstall": "npm run client:build",
    "start": "PRODUCTION=true npm run server:start",
    "lint": "eslint . --ext .js",
    "lint-and-fix": "eslint . --ext .js --fix"
  },
  "author": "N. van Urk",
  "license": "MIT",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.3.0",
    "dotenv-webpack": "^3.0.0",
    "eslint": "^7.9.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.11.2",
    "nodemon": "^2.0.4",
    "puppeteer": "^5.3.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "ammo.js": "github:kripken/ammo.js",
    "atob": "^2.1.2",
    "babel-loader": "^8.1.0",
    "blob-util": "^2.0.2",
    "cross-blob": "^2.0.0",
    "dotenv": "^8.2.0",
    "ecsy": "^0.4.2",
    "express": "^4.17.1",
    "sanitize-html": "^2.3.2",
    "three": "^0.126.1",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "winston-transport-browserconsole": "^1.0.5",
    "worker-interval": "^1.0.6",
    "ws": "^7.3.1",
    "xmlhttprequest": "^1.8.0"
  }
}
